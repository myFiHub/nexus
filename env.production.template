# Production Environment Variables Template
# Copy this file to .env.production and fill in your actual values

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id

# OneSignal Configuration
NEXT_PUBLIC_ONESIGNAL_APP_ID=your_onesignal_app_id_here

# Website Configuration
NEXT_PUBLIC_WEBSITE_LINK_URL=https://your-website-url.com

# Podium API Configuration
NEXT_PUBLIC_PODIUM_API_URL=https://your-podium-api-url.com/api/v1

# WebSocket Configuration
NEXT_PUBLIC_WEBSOCKET_ADDRESS=wss://your-websocket-url.com

# Outpost Server Configuration
NEXT_PUBLIC_OUTPOST_SERVER=your-outpost-server.com

# Luma API Configuration
NEXT_PUBLIC_LUMA_API_KEY=your_luma_api_key_here

# Web3Auth Configuration
NEXT_PUBLIC_WEB3AUTH_CLIENT_ID=your_web3auth_client_id_here

# Aptos/Movement Configuration
NEXT_PUBLIC_MOVEMENT_RPC_URL=https://your-movement-rpc-url.com/v1
NEXT_PUBLIC_PODIUM_PROTOCOL_ADDRESS=your_protocol_address_here
NEXT_PUBLIC_CHEER_BOO_ADDRESS=your_cheer_boo_address_here
NEXT_PUBLIC_FIHUB_ADDRESS=your_fihub_address_here
NEXT_PUBLIC_FIHUB_ADDRESS_APTOS=your_fihub_aptos_address_here

